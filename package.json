{
  "name": "realtime-rethinkdb",
  "version": "0.0.1",
  "description": "Realtime app to demonstrate Rethinkdb",
  "main": "server.js",
  "engines": {
    "node": ">=4.1.0 <5"
  },
  "scripts": {
    "start": "nf start",
    "start:prod": "NODE_ENV=production nf start db=1,web=1",
    "build": "npm run clean && npm run compile",
    "clean": "rimraf dist",
    "compile": "webpack --progress -p --config webpack.prod.config src/index.js"
  },
  "author": "Chris Atkins",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "babel": "^6.0.15",
    "babel-core": "^6.1.2",
    "babel-loader": "^6.0.1",
    "babel-preset-es2015": "^6.1.2",
    "babel-preset-react": "^6.1.2",
    "babel-preset-stage-0": "^6.1.2",
    "css-loader": "^0.22.0",
    "foreman": "^1.4.1",
    "nodemon": "^1.8.1",
    "react-hot-loader": "^1.3.0",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.3",
    "webpack-dev-server": "^1.12.1"
  },
  "dependencies": {
    "express": "^4.13.3",
    "lodash": "^3.10.1",
    "morgan": "^1.6.1",
    "normalize.css": "^3.0.3",
    "react": "^0.14.2",
    "react-dom": "^0.14.2",
    "rethinkdbdash": "^2.1.17",
    "socket.io": "^1.3.7",
    "socket.io-client": "^1.3.7"
  }
}
